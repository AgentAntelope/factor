<t:chloe xmlns:t="http://factorcode.org/chloe/1.0">

	<t:title>Photography</t:title>
	
	<table cellpadding="5" bgcolor="#999">
		<t:each t:name="rows">
			<tr>
				<t:bind-each t:name="value">
					<td><t:a t:href="$photos/pic" t:rest="id">
						<img src="@thumbnail"/>
					</t:a></td>
				</t:bind-each>
			</tr>
		</t:each>
	</table>
	
</t:chloe>